    function autofill_email() {
        var spans = document.getElementsByTagName("span");
        for (i = 0; i < spans.length; i++) {
            if (spans[i].className != "email_autofill")
                continue;
            spans[i].innerHTML = spans[i].innerHTML.replace(/ at /, "@");
        }
    }
 
    function page_onload() {
        autofill_email();
    }
